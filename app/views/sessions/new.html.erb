<div class="col-7 mx-auto">
  <h1><%= t('.tittle')%></h1>
  <%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <div for='field'>
    <%= f.label :email,(t 'attributes.email') %>
    <%= f.email_field :email, class: "form-control" %>
  </div>
  <div class='field'>
    <%= f.label :password,(t 'attributes.password') %>
    <%= f.password_field :password, class: "form-control" %>
  </div>
  <div class="actions mt-3 text-right">
    <%= f.submit t('application.new_session'), id:'create-session', class: "btn btn-primary" %>
  </div>
  <% end %>
</div>