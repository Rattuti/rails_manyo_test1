<p id="notice"><%= notice %></p>
<h1><%= t(".tittle") %></h1>

<p><strong><%= t("attributes.tittle") %>:</strong> <%= @task.tittle %></p>
<p><strong><%= t("attributes.content") %>:</strong> <%= @task.content %></p>
<p><strong><%= t("attributes.created_at") %>:</strong> <%= @task.created_at %></p>
<p><strong><%= t("attributes.created_at") %>:</strong> <%= @task.deadline_on %></p>
<p><strong><%= t("attributes.priority") %>:</strong> <%= @task.priority %></p>
<p><strong><%= t("attributes.status") %>:</strong> <%= @task.status %></p>
<p><strong><%= t("attributes.label") %>:</strong> 
  <% @task.labels.each do |label| %>
    <%= label.name %>
  <% end %></p>

<%= link_to t("attributes.edit"), edit_task_path(@task) %> |
<%= link_to t("attributes.back"), tasks_path ,id:"back"%>
